include ../search/search
include ../drop-down/drop-down

mixin search-drop-down-container
    .search-drop-down-container
        +title('search / drop down')
        .search-drop-down-container__search
            +search
            +search
        .search-drop-down-container__drop-down
            +drop-down({
                name: 'defaultSelect', 
                values: ['Choose An Option', 'This is option 1', 'This is option 2', 'This is option 3', 'This is option 4']
            })  