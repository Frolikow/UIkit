include ../form-feedback/form-feedback
include ../toggles/toggles
include ../tick-box/tick-box 

mixin form-elements
    +title('form elements')
    .form-elements
        .form-elements__form
            +form-feedback
        .form-elements__toggles-and-tick-box
            +title('toggles')
            .form-elements__toggles
                +toggles
                +toggles
            +title('tick boxes')
            .form-elements__tick-box
                +tick-box({
                    name: '', 
                    value: ''
                })
                +tick-box({
                    name: '', 
                    value: ''
                })