mixin step-slider(data)
  .step-slider.js-step-slider.ui-slider.ui-slider-handle.ui-slider-range(data-minimum = data.minimum, data-default = data.value, data-maximum = data.maximum, data-step = data.step)
  .step-slider__scale
    .step-slider__value 0
    .step-slider__value 25
    .step-slider__value 50
    .step-slider__value 75
    .step-slider__value 100



