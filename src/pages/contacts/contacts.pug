include ./../page-modules/header/header
include ./../page-modules/footer/footer
include ./../../modules/form-feedback/form-feedback
include ./../../modules/location/location

doctype html
html(lang = "ru")
    head
        meta(charset = "utf-8")
        title Контакты(UIkit)
        meta(NAME="keywords" CONTENT="аренда машин, томск, допустим")
        meta(NAME="description" CONTENT="Контакты")
        meta(name="viewport" content="initial-scale=1.0, width=device-width")
        meta(http-equiv = "Cache-Control" content = "no-cache")
    body
        .layout
            +header
            .main
                .contact-page-content
                    .contact-page-content__feedback
                        p.contact-page-content__text По любому вопросу вы можете обратиться через форму ниже
                        +form-feedback
                    .contact-page-content__address
                        p.contact-page-content__text Мы находимся здесь:
                        +location({
                            longitude: 56.4531, 
                            latitude: 84.975643
                        })
            +footer

script(async defer src = 'https://maps.googleapis.com/maps/api/js?key=AIzaSyBwCKcdCMMpMMqw5VEtVFyB5BLh467I3xY&callback=initMap')  